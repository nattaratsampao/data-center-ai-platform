{
  "models": [
    {
      "name": "Anomaly Detection",
      "type": "isolation_forest",
      "dataset": "sensor_timeseries.csv",
      "target": "is_anomaly",
      "features": ["temperature", "humidity", "power_consumption", "vibration"],
      "hyperparameters": {
        "n_estimators": 100,
        "contamination": 0.05,
        "max_features": 1.0
      },
      "evaluation_metrics": ["precision", "recall", "f1_score", "roc_auc"]
    },
    {
      "name": "Predictive Maintenance",
      "type": "xgboost_classifier",
      "dataset": "maintenance_labels.csv",
      "target": "needs_maintenance",
      "features": [
        "cpu_usage",
        "memory_usage",
        "temperature",
        "vibration",
        "power_consumption",
        "error_rate",
        "uptime_days"
      ],
      "hyperparameters": {
        "max_depth": 6,
        "learning_rate": 0.1,
        "n_estimators": 200,
        "scale_pos_weight": 4
      },
      "evaluation_metrics": ["accuracy", "precision", "recall", "f1_score"]
    },
    {
      "name": "Failure Type Classifier",
      "type": "random_forest_classifier",
      "dataset": "failure_events.csv",
      "target": "failure_type",
      "features": ["pre_cpu_avg", "pre_memory_avg", "pre_temperature_avg", "pre_vibration_avg", "pre_error_rate"],
      "hyperparameters": {
        "n_estimators": 150,
        "max_depth": 10,
        "min_samples_split": 5
      },
      "evaluation_metrics": ["accuracy", "confusion_matrix", "classification_report"]
    },
    {
      "name": "Workload Forecaster",
      "type": "lstm",
      "dataset": "workload_patterns.csv",
      "target": "total_requests",
      "features": ["total_requests", "cpu_utilization", "memory_utilization", "active_connections"],
      "hyperparameters": {
        "sequence_length": 24,
        "lstm_units": 64,
        "dropout": 0.2,
        "epochs": 50,
        "batch_size": 32
      },
      "evaluation_metrics": ["rmse", "mae", "mape"]
    },
    {
      "name": "Health Score Predictor",
      "type": "neural_network",
      "dataset": "server_performance.csv",
      "target": "health_score",
      "features": ["cpu_usage", "memory_usage", "disk_io", "network_throughput", "response_time", "error_rate"],
      "hyperparameters": {
        "layers": [64, 32, 16],
        "activation": "relu",
        "optimizer": "adam",
        "loss": "mse",
        "epochs": 100
      },
      "evaluation_metrics": ["rmse", "mae", "r2_score"]
    }
  ],
  "feature_engineering": {
    "rolling_windows": [6, 12, 24],
    "lag_features": [1, 3, 6, 12],
    "time_features": ["hour", "day_of_week", "is_business_hours", "is_weekend"]
  },
  "data_preprocessing": {
    "scaling": "standard_scaler",
    "handle_missing": "forward_fill",
    "outlier_detection": "iqr_method"
  }
}
